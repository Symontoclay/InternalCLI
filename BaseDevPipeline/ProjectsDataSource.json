      "Path": "%BASE_PATH%/SymOntoClayAsset",
      "License": "MIT",
      "SourcePath": "%SLN_ROOT_PATH%/Assets/SymOntoClay"
    },
    {
      "Kind": "ReleaseMngrSolution",
      "Href": "https://github.com/Symontoclay/ReleaseMngr",
      "Path": "%BASE_PATH%/ReleaseMngr",
      "License": "MIT"
    }
  ],
  "Artifacts": [
    {
      "Kind": "ProjectSite",
      "Path": "%BASE_PATH%/symontoclay.github.io"
    },
    {
      "Kind": "UnityPackage",
      "Path": "%USERPROFILE%/Documents/MyFirstSymOntoClayAsset_2.unitypackage"
    }
  ],
  "Licenses": [
    {
      "Name": "MIT",
      "FileName": "%APPDIR%/MITLicense.txt"
    }
