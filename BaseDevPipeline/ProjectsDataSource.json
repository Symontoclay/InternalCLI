        {
          "Kind": "Library",
          "Path": "SymOntoClayProjectFiles"
        },
        {
          "Kind": "Library",
          "Path": "SymOntoClayDefaultCLIEnvironment"
        },
        {
          "Kind": "CLI",
          "Path": "SymOntoClayCLI"
        },
        {
          "Kind": "IntegrationTest",
          "Path": "SymOntoClayUnity3DAssetTest"
        },
        {
          "Kind": "AdditionalApp",
          "Path": "TestSandbox"
        },
        {
          "Kind": "AdditionalApp",
          "Path": "LinguisticVariableViewer"
        },
        {
          "Kind": "CorePlugin",
          "Path": "SymOntoClaySoundBuses"
        }
      ]
    },
    {
