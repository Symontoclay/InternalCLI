<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
	  <TargetFramework>net8.0</TargetFramework>
	  <ImplicitUsings>enable</ImplicitUsings>
	  <Nullable>disable</Nullable>
	  <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
	  <Version>0.5.4.9</Version>
	  <PackageLicenseExpression>MIT</PackageLicenseExpression>
	  <AssemblyVersion>0.5.4.9</AssemblyVersion>
	  <FileVersion>0.5.4.9</FileVersion>
	  <GenerateDocumentationFile>True</GenerateDocumentationFile>
	  <DocumentationFile>SymOntoClay.CLI.Helpers.xml</DocumentationFile>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
    <PackageReference Include="NLog" Version="5.1.4" />
    <PackageReference Include="System.Configuration.ConfigurationManager" Version="4.7.0" />
    <PackageReference Include="SymOntoClay.Common" Version="0.5.4.9" />
  </ItemGroup>
</Project>